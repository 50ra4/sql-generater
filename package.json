{
  "name": "sql-query-builder-like-firestore",
  "version": "1.0.12",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest -c ./jest.config.js --watchAll",
    "test:nowatch": "jest -c ./jest.config.js",
    "clean": "rm -rf ./dist",
    "patch": "npm version patch && npm publish",
    "republish": "npm run test:nowatch && npm run clean && npm run build && npm run patch",
    "docs": "typedoc --out ./docs --target ES6 --mode file ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shigarashi1/sql-generater.git"
  },
  "author": "sigarashi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shigarashi1/sql-generater/issues"
  },
  "homepage": "https://github.com/shigarashi1/sql-generater#readme",
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*": "prettier --write",
    "*.{ts,tsx}": [
      "tslint -p ./tsconfig.json --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/sqlstring": "^2.2.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "sql",
    "query",
    "builder",
    "firestore",
    "tools",
    "node.js",
    "typescript"
  ],
  "dependencies": {
    "sqlstring": "2.3.1"
  }
}
